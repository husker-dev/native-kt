    [Critical] boolean jvmci5(int a1, long a2, int a3, long a4, int a5, long a6, int a7, int a8, long a9);
    [Critical] boolean jvmci6(float a1, float a2, float a3, float a4, float a5, float a6, float a7, float a8, float a9, int a10, int a11, int a12, int a13);
    [Critical] boolean jvmci7(float a1, double a2, float a3, double a4, float a5, double a6, float a7, float a8, double a9);
    [Critical] boolean jvmci8(int a1, double a2, float a3, long a4);
    [Critical] boolean jvmci9(int a1, double a2, float a3, long a4, long a5, double a6, float a7, float a8, int a9);
    [Critical] boolean jvmci10(string a1, double a2, float a3, long a4, long a5, double a6, string a7, float a8, int a9);
    [Critical] boolean jvmci11(float a1, int a2, float a3, int a4, float a5, int a6, float a7, int a8, float a9, int a10, float a11, int a12, float a13, int a14, float a15, int a16, float a17);
    [Critical] int jvmci12();
